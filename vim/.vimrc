filetype plugin indent off

set  hlsearch

syntax on
set number
set showtabline=2

set paste
set cursorline
hi Comment ctermfg=103
hi CursorLine term=none cterm=none ctermbg=17 guibg=black
set showcmd
set whichwrap=b,s,h,l,<,>,~,[,]
set nowrap
set ruler
set showmatch

filetype plugin indent on

syntax enable
colorscheme desert
